<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/widget0"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <RelativeLayout
        android:id="@+id/widget61"
        android:layout_width="match_parent"
        android:layout_height="76px"
        android:layout_x="0px"
        android:layout_y="0px"
        android:background="#FFFFFF" >

        <TextView
            android:id="@+id/widget62"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="Gangguan Mood"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" >
        </TextView>
    </RelativeLayout>

    <Button
        android:id="@+id/tambahgangguan_btn"
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:layout_x="60dip"
        android:layout_y="120dip"
        android:text="Tambah" >

    </Button>

    <Button
        android:id="@+id/editgangguan_btn"
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:layout_below="@id/tambahgangguan_btn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_x="60dip"
        android:layout_y="180dip"
        android:text="Edit" >

    </Button>

    <Button
        android:id="@+id/editgangguan_btn1"
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:layout_below="@id/editgangguan_btn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_x="60dip"
        android:layout_y="240dip"
        android:text="Hapus" >

    </Button>

    <Button
        android:id="@+id/backgangguan_btn"
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:layout_below="@id/editgangguan_btn1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_x="60dip"
        android:layout_y="300dip"
        android:text="Back" >

    </Button>

</RelativeLayout>